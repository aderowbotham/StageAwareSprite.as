package {	import com.aderowbotham.utils.StageAwareSprite;			/**	 * @author ade	 * Demo shows three example uses of addedToStage, removedFromStage and stageResize	 * replace contents of these methods with anything you like	 */	 	public class Demo extends StageAwareSprite {				public function Demo() {						super();		}		override protected function addedToStage():void{						trace("added to stage, stageWidth = "+stageWidth,", stageHeight = "+stageHeight);									this.x = this.y = 200;					}				override protected function removedFromStage():void{			trace("removed from stage");		}				override protected function stageResize():void{			trace("stage resized to: "+stageWidth+", "+stageHeight);			this.rotation +=5;		}			}}